index.js
function calcular (){
    let n1 = Number(window.prompt('Digite um número: '));
    let n2 = Number(window.prompt('Digite um número: '));
    let mensagem = window.document.getElementById("mensagem");

    mensagem.innerHTML = `<p>
    ${n1} + ${n2} é ${n1+n2}!</p>`
}
Ex7
index.html
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ecercício 07</title>
</head>
<body>
    <h1>Trabalhando com números</h1>
    <button onclick="calcular()">Clique aqui para inserir os dados</button>
    <p id="mensagem"></p>
</body>
<script src="index.js"></script>
</html>
index.js
function calcular (){
    let nome = window.prompt('Qual o seu nome');
    let n1 = Number(window.prompt('Digite um número: '));
    let n2 = Number(window.prompt('Digite um número: '));
    let n3 = Number(window.prompt('Digite um número: '));
    let mensagem = window.document.getElementById("mensagem");

    mensagem.innerHTML = `
    <p>Nome do aluno: ${nome}</p>
    <p>primeira nota: ${n1}</p>
    <p>segunda nota: ${n2}</p>
    <p>terceira nota: ${n3}</p>
    <p>média final: ${(n1+n2+n3)/3}!</p>`
}
Ex8
index.html
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ecercício 08</title>
</head>
<body>
    <h1>varios cálculos</h1>
    <button onclick="calcular()">Clique aqui para inserir um valor!</button>
    <p id="mensagem"></p>
</body>
<script src="index.js"></script>
</html>
index.js
function calcular(){
    let n1 = Number(window.prompt('Digite um número: '));
    const raiz = Math.sqrt(n1);
    console.log(raiz);
    const raizCubica = Math.cbrt(n1);
    console.log(raizCubica);
    const parteInteira = Math.parseInt(n1);
    console.log(parteInteira);
    const valorInteiro = Math.round(n1);
    console.log(valorInteiro);
    let mensagem = window.document.getElementById("mensagem");

    mensagem.innerHTML = `<p>Número: ${n1}</p>
    <hr>
    <p>O seu valor absoluto é: ${n1}</p>
    <p>A sua parte inteira é: ${parteInteira}</p>
    <p>O valor inteiro mais próximo é: ${valorInteiro}</p>
    <p>A sua raiz quadrada é: ${raiz}</p>
    <p>A sua raiz cúbica é: ${raizCubica}</p>
    <p>O valor de ${n1}²: ${n1*n1}</p>
    <p>O valor de ${n2}³: ${(n1*n1*n1)}!</p>`

} 
