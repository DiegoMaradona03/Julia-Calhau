Ex10
index.html
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Exercício 02</title>
</head>
<body>
    <h1>Interagindo com o botão</h1>
    <button onclick="clicou1()">Ação 1</button>
    <button onclick="clicou2()">Ação 2!</button>
    <button onclick="clicou3()">Ação 3</button>
    <button onclick="clicou4()">Ação 4</button>
    <button onclick="clicou5()">Clique aqui para reiniciar!</button>
    <p id="mensagem"></p>
</body>
<script src="index.js"></script>
</html>
index.js
function clicou1 (){
    let mensagem = window.document.getElementById("mensagem");

    mensagem.innerHTML = `<p>
    Clicou no primeiro botão</p>`
}
function clicou2 (){
    let mensagem = window.document.getElementById("mensagem");

    mensagem.innerHTML = `<p>
    Clicou no segundo botão</p>`
}
function clicou3 (){
    let mensagem = window.document.getElementById("mensagem");

    mensagem.innerHTML = `<p>
    Clicou no terceiro botão</p>`
}
function clicou4 (){
    let mensagem = window.document.getElementById("mensagem");

    mensagem.innerHTML = `<p>
    Clicou no quarto botão</p>`
}
function clicou5 (){
    let mensagem = window.document.getElementById("mensagem");

    mensagem.innerHTML = ``
}
