index.js
function início(){
    let nome = window.prompt("Qual é o seu nome?")
    window.alert(`Olá, ${nome}! é um prazer te conhecer!`)
}
Ex4
index.html
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Exercício 04</title>
</head>
<body>
    <h1>Trabalhando com dados V2.0</h1>
    <button onclick="clicou()">Clique aqui!</button>
    <p id="mensagem"></p>
</body>
<script src="index.js"></script>
</html>
index.js
function clicou(){
    let nome = window.prompt('Qual o seu nome');
    let mensagem = window.document.getElementsById('mensagem');

    mensagem.innerHTML = `<p>Olá, <strong>${nome}</strong>! É
    prazer em te conhecer</p>`
}
Ex5
index.html
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ecercício 05</title>
</head>
<body>
    <h1>Trabalhando com números</h1>
    <button onclick="calcular()">Clique aqui para inserir um valor!</button>
    <p id="mensagem"></p>
</body>
<script src="index.js"></script>
</html>
index.js
function calcular (){
    let n1 = Number(window.prompt('Digite um número: '));
    let mensagem = window.document.getElementById("mensagem");

    mensagem.innerHTML = `<p> o dobro de
    ${n1} é ${n1*2}, e a metade é ${n1/2}!</p>`
}
